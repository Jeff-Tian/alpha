    "dva": "^2.4.0",
    "egg": "^2.6.1",
    "egg-alinode": "^2.0.1",
    "egg-oauth2-server": "^2.2.6",
    "egg-passport": "^2.1.0",
    "egg-passport-citi": "0.2.3",
    "egg-passport-github": "^1.0.0",
    "egg-passport-local": "^1.2.1",
    "egg-passport-wechat-ts": "latest",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^5.1.0",
    "egg-useragent-ts": "0.0.1",
    "egg-view": "^2.1.2",
    "egg-view-assets": "^1.0.0",
    "egg-view-pug": "^1.0.0",
    "enquire-js": "0.2.1",
