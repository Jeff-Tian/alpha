{
  "name": "alpha",
  "version": "1.0.0",
  "description": "I am Alpha, I am Omega.",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-alpha",
    "stop": "egg-scripts stop --title=egg-server-alpha",
    "dev": "NODE_ENV=local EGG_SERVER_ENV=local egg-bin dev",
    "debug": "egg-bin debug",
    "test-with-full-trace": "egg-bin test TESTS test/app/controller/passport-local.test.ts --full-trace",
    "test-local": "npm run migrate&&egg-bin test",
    "test-local-changed": "npm run migrate&&egg-bin test --changed",
    "test": "sh test/mysql-up.sh&&npm run lint -- --fix && npm run test-local-changed",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run migrate && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean",
    "migrate": "npx sequelize db:migrate",
    "log": "heroku logs --tail -a uniheart",
    "build": "APP_ROOT=$PWD/app/web umi build",
    "sync": "./script/sync.js"
  },
  "dependencies": {
    "@antv/data-set": "^0.9.6",
    "@babel/runtime": "^7.1.2",
    "antd": "^3.10.0",
    "bizcharts": "^3.2.2",
