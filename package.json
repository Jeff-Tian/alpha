{
  "name": "@jeff-tian/alpha",
  "version": "0.0.0-development",
  "description": "I am Alpha, I am Omega.",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --workers=1 --daemon --title=egg-server-alpha",
    "stop": "docker-compose down &&egg-scripts stop --title=egg-server-alpha",
    "dev": "npm run start:dev-env&&cross-env NODE_ENV=local EGG_SERVER_ENV=local egg-bin dev",
    "debug": "egg-bin debug",
    "test-with-full-trace": "egg-bin test TESTS test/app/controller/proxy.test.ts --full-trace",
    "test": "npm run migrate&&egg-bin test",
    "test-local-changed": "yarn start:dev-env&&npm run migrate&&egg-bin test --changed",
    "test-local": "npm run lint -- --fix && npm test",
    "start:dev-env": "docker-compose up -d --remove-orphans",
    "stop:dev-env": "docker-compose down",
    "test:frontend": "cd app/web && umi test && cd ../.. || cd ../..",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run test:frontend && npm run migrate && npm test && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts --fix",
    "clean": "ets clean",
    "migrate": "npx sequelize db:migrate",
    "log": "heroku logs --tail -a uniheart",
    "build": "ets && tsc -p tsconfig.json && npx sequelize db:migrate --url $CLEARDB_DATABASE_URL && APP_ROOT=$PWD/app/web umi build",
    "sync": "./script/sync.js",
    "tplant": "tplant --input app/model/**/*.ts --output models.svg",
    "semantic-release": "semantic-release",
    "i:taobao": "npm i --registry=https://registry.npm.taobao.org",
    "postinstall": "umi g tmp"
  },
